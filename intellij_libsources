#!/bin/bash

. $(dirname ${BASH_SOURCE[0]})/intellij_common

checknode
setupsdk
setuplibs

"$NODEJS" $(dirname $0)/bin/pom.js $PWD $@

mvn dependency:sources
grep '<module>.*</module>' pom.xml | awk -F'[<>]' '{ print $3 "/pom.xml" }' | xargs rm
rm pom.xml

"$NODEJS" $(dirname $0)/bin/run.js $PWD $@

ignoreiml